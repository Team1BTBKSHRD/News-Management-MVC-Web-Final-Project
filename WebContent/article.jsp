                    <!-- news publish date -->
                    <p class="col-sm-6 article-info pull-left">
                      <strong >ចេញផ្សាយ 
                      <kbd><time>2015/03/07</time></kbd>
                      </strong> 
                    </p>
                    <!-- news category -->
                    <p class="col-sm-6 article-info pull-right" style="text-align: right;">
                      <strong>ប្រភេទព័ត៌មាន<kbd>ជិវិត  និង សង្គម</kbd> </strong>
                    </p>
